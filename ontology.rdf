        <rdfs:subPropertyOf rdf:resource="https://material-digital.de/pmdco/hasQuantitativeParameter"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://material-digital.de/pmdco/Object"/>
                    <rdf:Description rdf:about="https://material-digital.de/pmdco/Process"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
