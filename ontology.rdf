    <!-- https://glasdigi.cms.uni-jena.de/glass/hasCoolOven -->


    <owl:ObjectProperty rdf:about="https://glasdigi.cms.uni-jena.de/glass/hasCoolOven">
        <rdfs:subPropertyOf rdf:resource="https://material-digital.de/pmdco/hasProcessNodeComponent"/>
        <rdfs:domain rdf:resource="https://glasdigi.cms.uni-jena.de/glass/GlassManufacturingProcessNode"/>
        <rdfs:range rdf:resource="https://glasdigi.cms.uni-jena.de/glass/CoolOven"/>
        <rdfs:label xml:lang="en">has cool oven</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://glasdigi.cms.uni-jena.de/glass/hasCrucible -->


    <owl:ObjectProperty rdf:about="https://glasdigi.cms.uni-jena.de/glass/hasCrucible">
        <rdfs:subPropertyOf rdf:resource="https://material-digital.de/pmdco/hasProcessNodeComponent"/>
        <rdfs:domain rdf:resource="https://glasdigi.cms.uni-jena.de/glass/GlassManufacturingProcessNode"/>
        <rdfs:range rdf:resource="https://glasdigi.cms.uni-jena.de/glass/crucible"/>
        <rdfs:label xml:lang="en">has crucible</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://glasdigi.cms.uni-jena.de/glass/hasFurnace -->


    <owl:ObjectProperty rdf:about="https://glasdigi.cms.uni-jena.de/glass/hasFurnace">
        <owl:equivalentProperty rdf:resource="https://glasdigi.cms.uni-jena.de/glass/hasOven"/>
        <rdfs:subPropertyOf rdf:resource="https://material-digital.de/pmdco/hasProcessNodeComponent"/>
        <rdfs:domain rdf:resource="https://glasdigi.cms.uni-jena.de/glass/GlassManufacturingProcessNode"/>
        <rdfs:range rdf:resource="https://glasdigi.cms.uni-jena.de/glass/oven"/>
        <rdfs:label xml:lang="en">has furnace</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://glasdigi.cms.uni-jena.de/glass/hasInputBatch -->


    <owl:ObjectProperty rdf:about="https://glasdigi.cms.uni-jena.de/glass/hasInputBatch">
        <owl:equivalentProperty rdf:resource="https://glasdigi.cms.uni-jena.de/glass/hasInputRawMaterial"/>
        <obo:IAO_0000119 xml:lang="en">https://en.wikipedia.org/wiki/Glass_batch_calculation</obo:IAO_0000119>
        <rdfs:comment xml:lang="en">The raw materials mixture for glass melting is termed &quot;batch&quot;. The batch must be measured properly to achieve a given, desired glass formulation.</rdfs:comment>
        <rdfs:label xml:lang="en">has input batch</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://glasdigi.cms.uni-jena.de/glass/hasInputRawMaterial -->


    <owl:ObjectProperty rdf:about="https://glasdigi.cms.uni-jena.de/glass/hasInputRawMaterial">
        <rdfs:domain rdf:resource="https://material-digital.de/pmdco/Process"/>
        <rdfs:range rdf:resource="https://glasdigi.cms.uni-jena.de/glass/RawMaterial"/>
        <rdfs:comment xml:lang="en">e.g. Na2CO3 (Na2O), Al2O3, H3BO3 (B2O3), SiO2, etc.</rdfs:comment>
        <rdfs:label xml:lang="en">has input raw material</rdfs:label>
    </owl:ObjectProperty>
    


